<Application x:Class="ZNO.Front.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:locator="clr-namespace:ZNO.Front.ViewModel.Locator"
             xmlns:vm="clr-namespace:ZNO.Front.ViewModel"
             Startup="App_Startup">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                    <ResourceDictionary Source="Styles/Colors.xaml" />
                    <ResourceDictionary Source="Styles/Button.xaml"/>
                    <ResourceDictionary Source="Styles/TextBlock.xaml"/>
                    <ResourceDictionary Source="Styles/ComboBox.xaml"/>  
            </ResourceDictionary.MergedDictionaries>
                <locator:ViewModelLocator xmlns="clr-namespace:ZNO.Front.ViewModel.Locator" x:Key="ViewModelLocator" />
        </ResourceDictionary>
    </Application.Resources>
</Application>
